# Redis Configuration
# For local development
REDIS_HOST=localhost
REDIS_PORT=6379

# For Toolforge deployment
# REDIS_HOST=redis.svc.tools.eqiad1.wikimedia.cloud
# REDIS_PORT=6379

# CRITICAL: Generate a unique prefix for Toolforge multi-tenant environment
# Generate with: openssl rand -base64 32
# This prevents key collisions with other tools
REDIS_KEY_PREFIX=dev:

# Cache TTL settings (seconds)
USER_CACHE_TTL=3600
COURSE_CACHE_TTL=86400
COURSE_USERS_CACHE_TTL=3600

# Outreach Dashboard base URL
OUTREACH_BASE_URL=https://outreachdashboard.wmflabs.org

# Application settings
LOG_LEVEL=INFO
